    var registryRetailer = function () {
        if ((typeof RRUWRegInfo) == 'undefined') return;
        RR.wait('api', RRUWRegInfo, function (data) {
            if (RRUWRegInfo.affiliateId == 950 || RRUWRegInfo.affiliateId == 994) {
                var retailerHtml = [];
                for (var key in data) {
                    if (data.length > 0 )
                      retailerHtml.push('<li><a href="http://braxtonanddanielle.ourwedding.com/view/8476593777902968/'&#32;+&#32;data[key].RegistryGVR&#32;+&#32;'" target="_blank">' + data[key].Retailer + '</a></li>');
                }
                if (retailerHtml.join("") == "") return;
                var registries2Html = ['<div id="RegistryList2"><ul>', retailerHtml.join(""), '</ul></div>'].join("");
                $("#navLeft ul").find("li#7").eq(0).append(registries2Html);
            }
        });
    };
    $("#navLeft ul").find("li#7").length && registryRetailer();
